package com.tahir.functionalprogramming.fi;

import java.util.List;
import java.util.function.Function;
import java.util.stream.Collectors;

public class FI07MethodReferencing {

	public static void main(String[] args) {
		List<String> courses = List.of("Spring", "Spring Boot", "API", "Microservices","Spring Boot", "AWS", "PCF", "Azure", "Docker",
				"Kubernetes");
		courses.stream().map(s->s.toUpperCase()).collect(Collectors.toMap(Function.identity(), String::toUpperCase));

	}

}
